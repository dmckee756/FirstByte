<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="headlineTextFB">
        <item name="android:textAppearance">@font/rasa</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textSize">@dimen/search_text_Size</item>
        <item name="android:shadowColor">@color/textShadow</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>

    <style name="appBarTextFB">
        <item name="android:textAppearance">@font/roboto</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textSize">@dimen/appBarTextsSze</item>
        <item name="android:shadowColor">@color/textShadow</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">1</item>
    </style>


    <style name="layoutSectionHeaderText">
        <item name="android:textAppearance">@font/roboto</item>
        <item name="android:textColor">@color/layoutOutline</item>
        <item name="android:textSize">@dimen/layoutSectionTextSize</item>
    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="color">@color/iconColor</item>
    </style>

</resources>